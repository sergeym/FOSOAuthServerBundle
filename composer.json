{
    "name": "novosga/oauth-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony4 OAuth Server Bundle",
    "keywords": ["oauth", "oauth2", "server"],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
          "name": "Arnaud Le Blanc",
          "email": "arnaud.lb@gmail.com"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/FriendsOfSymfony/FOSOAuthServerBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "friendsofsymfony/oauth2-php": "~1.1",
        "symfony/framework-bundle": "^3.3",
        "symfony/security-bundle": "^3.3"
    },
    "require-dev": {
        "symfony/class-loader": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/form": "^3.3",
        "symfony/console": "^3.3",
        "willdurand/propel-typehintable-behavior": "^1.0.4",
        "propel/propel1": "^1.6.5",
        "phing/phing": "~2.4",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/orm": "~2.3"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "*",
        "doctrine/mongodb-odm-bundle": "*",
        "propel/propel-bundle": "If you want to use Propel with Symfony2, then you will have to install the PropelBundle",
        "willdurand/propel-typehintable-behavior": "The Typehintable behavior is useful to add type hints on generated methods, to be compliant with interfaces",
        "symfony/form" : "Needed to be able to use the AuthorizeFormType",
        "symfony/console": "Needed to be able to use commands"
    },
    "autoload": {
        "psr-4": { "FOS\\OAuthServerBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
